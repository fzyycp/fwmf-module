<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--微信支付请求就来 -->
    <bean id="weixinPayRecordsService" class="cn.faury.fwmf.module.service.payment.service.WeixinPayRecordsServiceImpl">
        <constructor-arg name="commonDao" ref="commonDao"/>
    </bean>
    <!--微信支付回调记录 -->
    <bean id="weixinCallbackRecordsService" class="cn.faury.fwmf.module.service.payment.service.WeixinCallbackRecordsServiceImpl">
        <constructor-arg name="commonDao" ref="commonDao"/>
    </bean>

    <!--支付宝支付请求就来 -->
    <bean id="alipayRecordsService" class="cn.faury.fwmf.module.service.payment.service.AlipayRecordsServiceImpl">
        <constructor-arg name="commonDao" ref="commonDao"/>
    </bean>
    <!--支付宝支付回调记录 -->
    <bean id="alipayCallbackRecordsService" class="cn.faury.fwmf.module.service.payment.service.AlipayCallbackRecordsServiceImpl">
        <constructor-arg name="commonDao" ref="commonDao"/>
    </bean>
</beans>